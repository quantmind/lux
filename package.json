        "angular-mocks": "1.5",
        "babel-eslint": "6.1",
        "babel-preset-es2015": "6.13",
        "babel-preset-es2015-loose-rollup": "7.0",
        "babelify": "7.3",
        "browserify": "13.1",
        "browserify-istanbul": "2.0",
        "d3-collection": "1.0",
